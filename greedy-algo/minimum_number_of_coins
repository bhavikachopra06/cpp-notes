class Solution {
  public:
    int findMin(int n) {
        vector<int>arr = {10,5,2,1};
        int i = 0;
        int notes = 0;
        int count = 0;
        while(n!=0 && i<arr.size()){
            notes = n/arr[i];
            count = count + notes;
            n = n%arr[i];
            i++;
        }
        if(n!=0) return -1;
        return count;
    }
};
